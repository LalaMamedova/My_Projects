@using QuizLib.Dto.Request
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@model ReqisterRequest;
@{
    var info = ViewData["info"];
}
<div class="d-block col-2">

    <form asp-action="SigninAction" asp-controller="User" method="post">
        <input required asp-for="Email" type="email" placeholder="Email" />
        <input required asp-for="Password" type="password" placeholder="Password" />
        <button type="submit">Sign in!</button>
    </form>
    <h1>@info</h1>
</div>